#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define prev a

int n;

int main() {
	
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	
	cin >> n;
	int i = 666;
	
	while (n != 0) {
		if (to_string(i).find("666") != string::npos) {
			n--;
		}
		i++;
	}
	cout << --i << "\n";

	return 0;
}

